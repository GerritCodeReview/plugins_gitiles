[access]
	inheritFrom = Public-Plugins
[project]
	description = Plugin running Gitiles alongside a Gerrit server
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group gerrit-verifiers
	submit = group gerrit-verifiers
	exclusiveGroupPermissions = submit
[access "refs/tags/*"]
	createTag = group gerrit-verifiers
[access "refs/*"]
	owner = group google/gerritcodereview-maintainers@googlegroups.com
